{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@now/node"
      }
    ],
    "routes": [
      
        {
            "src": "/anime",
            "dest": "/routes/anime.js"
          },
          {
            "src": "/anime-details",
            "dest": "/routes/anime-details.js"
          }, 
          {
            "src": "/(.*)",
            "dest": "/routes/index.js"
          }

    ]
}